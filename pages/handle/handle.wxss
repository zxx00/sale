/* pages/handle/handle.wxss */

page {
  width: 100%;
  height: 100%;
  background: #f7f7f7;
}
.shouq{
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 30%;
}
.userinfo-avatar{
  width: 180rpx;
  height: 180rpx
}
.no_data image {
  width: 230rpx;
  height: 245rpx;
}

.no_data {
  text-align: center;
   padding-top: 400rpx;
}

.no_data view {
  margin-top: 40rpx;
  font-size: 30rpx;
  font-family: Source Han Sans CN;
  color: rgba(153, 153, 153, 1);
}

.conter {
  width: 690rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 8rpx 7rpx 0px rgba(172, 166, 183, 0.09);
  border-radius: 10rpx;
  margin: 0 auto;
  margin-top: 20rpx;
}

.conters {
  border-bottom: solid 1rpx #eceaea;
  padding: 30rpx 31rpx;
}

.conters text:nth-child(2) {
  position: absolute;
  right: 59rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  color: rgba(102, 102, 102, 1);
}

.conters text:nth-child(1) {
  font-size: 30rpx;
  font-family: PingFang SC;
  color: #141414;
}
